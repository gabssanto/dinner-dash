<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <body class="fundo">
    <div class="log">
      <h2 class="title">dinnerdash</h2>

      <div class="intro1">
        <%= f.label :email %><br />
        <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control" %>
      </div>

      <div class="intro2" >
        <%= f.label :senha %><br />
        <%=  f.password_field  :password, autocomplete: "off", class: "form-control" %>
      </div>

      <div class="intro4">
        <%= f.submit "fazer login", class: "btn btn-success intro4" %>
      </div>

      <div class="rodape">
        <h6 class="quest">NÃ£o possui conta?</h6>
      </div>
      <%= link_to "fazer meu cadastro", new_user_registration_path, class: "btn btn-outline-success shared"   %>


    </div>
    </body>
<% end %>


